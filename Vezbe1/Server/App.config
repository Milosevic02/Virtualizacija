<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<services>
			<service name="Service.LibraryService">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4002"
/>
					</baseAddresses>
				</host>
				<!-- Service Endpoints -->
				<endpoint
				address="ServiceLibrary"
				binding="netTcpBinding"
				contract="Common.ILibrary" />
			</service>
		</services>
	</system.serviceModel>
</configuration>
