<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<appSettings>
		<add key="downloadPath" value="DowloadFiles"/>
	</appSettings>
	<system.serviceModel>
		<client>
			<endpoint name="FileHandlingService"
			address="net.tcp://localhost:4000/Service"
			binding="netTcpBinding"
			contract="Common.IFileHandler" />
		</client>
	</system.serviceModel>
</configuration>